{
  "name": "express-webpack-full-live-reload-example",
  "version": "1.0.0",
  "description": "A workflow with full live reload for webpack&express application.",
  "repository": {
    "type": "git",
    "url": "https://github.com/swustdyd/express-webpack-full-live-reload-example.git"
  },
  "author": "aaron",
  "main": "app.js",
  "scripts": {
    "start": "cross-env NODE_ENV=dev supervisor -w server,app.js -e node,js,jade app",
    "start:flow": "cross-env NODE_ENV=dev supervisor -w server,app.js -e node,js,jade -x babel-node app.js",
    "browsersync": "cross-env NODE_ENV=dev node app_browsersync",
    "build": "cross-env NODE_ENV=production & webpack --config webpack.production.config.js -p",
    "production": "webpack --config webpack.production.config.js -p & cross-env NODE_ENV=production node app",
    "eslint": "eslint **/*.js",
    "eslint:fix": "eslint **/*.js --fix",
    "flow": "flow init"
  },
  "license": "MIT",
